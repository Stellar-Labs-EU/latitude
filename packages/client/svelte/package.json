{
  "name": "@latitude-sdk/svelte",
  "version": "0.0.1",
  "scripts": {
    "dev": "vite dev",
    "build:vite": "vite build",
    "build": "pnpm run build:vite && pnpm run package",
    "preview": "vite preview",
    "package": "pnpm run prepare && svelte-package && publint",
    "prepublishOnly": "pnpm run package",
    "prepare": "svelte-kit sync",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test": "vitest",
    "lint": "eslint .",
    "prettier": "prettier --write src/**/*.ts src/**/*.svelte",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    "./theme/preset": "./tailwind.config.js",
    "./latitude.css": "./src/assets/latitude.css",
    "./ui/*": {
      "types": "./dist/components/ui/*/index.d.ts",
      "import": "./dist/components/ui/*/index.js",
      "svelte": "./dist/components/ui/*/index.svelte"
    }
  },
  "files": [
    "src/assets/latitude.css",
    "tailwind.config.js",
    "dist",
    "!dist/**/*.test.*",
    "!dist/**/*.stories.svelte"
  ],
  "peerDependencies": {
    "svelte": "^4.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^7.6.16",
    "@storybook/addon-interactions": "^7.6.16",
    "@storybook/addon-links": "^7.6.16",
    "@storybook/addon-svelte-csf": "^4.1.1",
    "@storybook/addon-themes": "^7.6.16",
    "@storybook/blocks": "^7.6.16",
    "@storybook/svelte": "^7.6.16",
    "@storybook/sveltekit": "^7.6.16",
    "@storybook/test": "^7.6.16",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/package": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "publint": "^0.1.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.6.16",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.3",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.11",
    "vitest": "^1.2.0"
  },
  "dependencies": {
    "bits-ui": "^0.18.1",
    "clsx": "^2.1.0",
    "lucide-svelte": "^0.330.0",
    "tailwind-merge": "^2.2.1",
    "tailwind-variants": "^0.2.0"
  }
}
